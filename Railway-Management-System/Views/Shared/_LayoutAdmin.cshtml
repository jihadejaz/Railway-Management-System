<!DOCTYPE html>

<html>
<head>
    <meta charset="UTF-8">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, shrink-to-fit=no" name="viewport">
    <title>Ecommerce Dashboard &mdash; CodiePie</title>

    <link rel="stylesheet" href="~/admin/assets/modules/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/admin/assets/modules/fontawesome/css/all.min.css">

    <link rel="stylesheet" href="~/admin/assets/modules/jqvmap/dist/jqvmap.min.css">
    <link rel="stylesheet" href="~/admin/assets/modules/summernote/summernote-bs4.css">
    <link rel="stylesheet" href="~/admin/assets/modules/owlcarousel2/dist/assets/owl.carousel.min.css">
    <link rel="stylesheet" href="~/admin/assets/modules/owlcarousel2/dist/assets/owl.theme.default.min.css">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.0/css/all.css" integrity="sha384-lZN37f5QGtY3VHgisS14W3ExzMWZxybE1SJSEsQp9S+oqd12jhcu+A56Ebc1zFSJ" crossorigin="anonymous">

    <link rel="stylesheet" href="~/admin/assets/css/style.min.css">
    <link rel="stylesheet" href="~/admin/assets/css/components.min.css">
    <title>@ViewBag.Title</title>
</head>
<body class="layout-4">



    <div id="app">
        <div class="main-wrapper main-wrapper-1">
            <div class="navbar-bg"></div>

            <!-- Start app top navbar -->
            <nav class="navbar navbar-expand-lg main-navbar">
                <form class="form-inline mr-auto">
                    <ul class="navbar-nav mr-3">
                        <li><a href="#" data-toggle="sidebar" class="nav-link nav-link-lg"><i class="fas fa-bars"></i></a></li>
                    </ul>                 
                </form>   
                <ul>
                    <li class="dropdown">
                        <a href="#" data-toggle="dropdown" class="nav-link dropdown-toggle nav-link-lg nav-link-user">
                            <img alt="image" src="assets/img/avatar/avatar-1.png" class="rounded-circle mr-1">
                            <div class="d-sm-none d-lg-inline-block">@TempData.Peek("Data")</div>
                        </a>
                        <div class="dropdown-menu dropdown-menu-right">
                            <div class="dropdown-title">Logged in 5 min ago</div>
                            <a href="Admin_profile" class="dropdown-item has-icon"><i class="far fa-user"></i> Profile</a>
                            
                          
                            <div class="dropdown-divider"></div>
                            <a href="admin_logout" class="dropdown-item has-icon text-danger"><i class="fas fa-sign-out-alt"></i> Logout</a>
                        </div>
                    </li>
                </ul>
            </nav>

            <!-- Start main left sidebar menu -->
            <div class="main-sidebar sidebar-style-3">
                <aside id="sidebar-wrapper">
                    <div class="sidebar-brand">
                        <a href="Admin">RMS</a>
                    </div>
                    <div class="sidebar-brand sidebar-brand-sm">
                        <a href="Admin">RMS</a>
                    </div>
                    <ul class="sidebar-menu">
                        <li class="menu-header">Dashboard</li>
                        <li class="dropdown active">
                            <a href="Admin" class="nav-link has-dropdown"><i class="fas fa-fire text-danger"></i><span>Controls</span></a>
                            <ul class="dropdown-menu">
                                <li class="active"><a class="nav-link" href="show_user"><i class="fa fa-user"></i> <span>Passengers Data</span></a></li>
                            </ul>
                        </li>
                        <li class="menu-header">Upload Portals</li>

                        <li><a class="nav-link" href="Alluser">Top Navigation</a></li>
                        <li><a class="nav-link" href="Add_Train"><i class="fa fa-train"></i> <span>Add Trains</span></a></li>
                        <li><a class="nav-link" href="Add_station"><i class="fa fa-home"></i> <span>Add Stations </span></a></li>
                        <li class="menu-header">Settings</li>
     
                       
                      
                </aside>
            </div>


        </div>
    </div>

    <div>
        @RenderBody()
    </div>

    <script src="~/admin/assets/bundles/lib.vendor.bundle.js"></script>
    <script src="~/admin/js/CodiePie.js"></script>

    <!-- JS Libraies -->
    <script src="~/admin/assets/modules/jquery.sparkline.min.js"></script>
    <script src="~/admin/assets/modules/chart.min.js"></script>
    <script src="~/admin/assets/modules/owlcarousel2/dist/owl.carousel.min.js"></script>
    <script src="~/admin/assets/modules/summernote/summernote-bs4.js"></script>
    <script src="~/admin/assets/modules/chocolat/dist/js/jquery.chocolat.min.js"></script>

    <!-- Page Specific JS File -->
    <script src="admin/js/page/index.js"></script>

    <!-- Template JS File -->
    <script src="~/admin/js/scripts.js"></script>
    <script src="~/admin/js/custom.js"></script>

</body>
</html>
